dev tun
persist-tun
persist-key
data-ciphers AES-256-GCM:AES-128-GCM:CHACHA20-POLY1305:AES-256-CBC
data-ciphers-fallback AES-256-CBC
auth SHA256
tls-client
client
resolv-retry infinite
remote 10.0.2.15 1194 udp4
nobind
verify-x509-name "CA-VPN" name
pkcs12 VpnLocal-UDP4-1194-vpn-user.p12
tls-auth VpnLocal-UDP4-1194-vpn-user-tls.key 1
remote-cert-tls server
explicit-exit-notify

