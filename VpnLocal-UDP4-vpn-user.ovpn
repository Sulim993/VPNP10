dev tun
persist-tun
persist-key
#data-ciphers AES-256-GCM:AES-128-GCM:CHACHA20-POLY1305:AES-256-CBC
#data-ciphers-fallback AES-256-CBC
auth SHA256
tls-client
client
resolv-retry infinite
remote REMOTEIP 1194 udp4
nobind
verify-x509-name "CA-VPN" name
pkcs12 /etc/openvpn/client/VpnLocal-UDP4-opc.p12
tls-auth /etc/openvpn/client/VpnLocal-UDP4-opc-tls.key 1
remote-cert-tls server
explicit-exit-notify

