dev tun
persist-tun
persist-key
data-ciphers AES-256-GCM:AES-128-GCM:CHACHA20-POLY1305:AES-256-CBC
data-ciphers-fallback AES-256-CBC
auth SHA256
tls-client
client
resolv-retry infinite
remote 192.168.1.82 16400 udp4
nobind
verify-x509-name "opc" name
pkcs12 VpnLocal-UDP4-16400-vpn-user.p12
tls-auth VpnLocal-UDP4-16400-vpn-user-tls.key 1
remote-cert-tls server
explicit-exit-notify
auth-nocache
